@model List<string>
@{
    ViewBag.Title = "PackageUsers";
}

<div class="container" style="margin-top: 15px;">
    <div class="panel panel-default">
        <div class="row">
            <div class="col-md-12">
                <table class="table">
                    <thead>
                        <tr>
                            <th width="100px;">Action</th>
                            <th>User Email</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Any())
                        {
                            foreach (var user in Model)
                            {
                                <tr>
                                    <td>
                                        <a class="btn btn-danger btn-xs" href="@Url.Action("GoogleScript", "Googlepackage", new { email = user })">Add Google Script</a>
                                    </td>
                                    <td>
                                        @user
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


